{
  "name": "electron-express-react",
  "version": "1.0.0",
  "description": "electron-express-react",
  "homepage": "./",
  "main": "./app/public/electron.js",
  "scripts": {
    "start": "electron .",
    "dev": "webpack-dev-server --open --config ./webpack.dev.js",
    "build": "rimraf ./build && mkdir build && webpack --progress -p --config webpack.prod.js",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/public/electron.js",
    "preelectron-pack": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "electron": "^8.1.1",
    "electron-builder": "^22.4.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "path": "^0.12.7",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "build": {
    "appId": "com.example.electron-express-react",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
